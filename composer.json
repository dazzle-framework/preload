{
    "name": "dazzlesoftware/preload",
    "type": "library",
    "description": "Dazzle Preload Package",
    "keywords": ["dazzle", "framework", "console"],
    "homepage": "https://github.com/dazzle-framework/preload",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "fig/link-util": "^1.0",
        "psr/link": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/application": "^1.0|^2.0",
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "dazzlesoftware/di": "^1.0|^2.0",
        "dazzlesoftware/event": "^1.0|^2.0",
        "dazzlesoftware/uri": "^1.0|^2.0",
        "phpunit/phpunit": "^9.0",
        "symfony/web-link": "^3.4|^4.4|^5.0"
    },
    "suggest": {
        "dazzlesoftware/application": "To use the PreloadSubscriber event listener, install `^1.0`",
        "dazzlesoftware/di": "To use the PreloadProvider service provider, install `^1.0`",
        "dazzlesoftware/event": "To use the PreloadSubscriber event listener, install `^1.0`",
        "symfony/web-link": "To use the PreloadSubscriber event listener, install `^3.4|^4.4|^5.0`"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Preload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Preload\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
